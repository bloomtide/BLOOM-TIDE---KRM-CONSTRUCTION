# Copy this to .env to use a local MongoDB instead of the cloud DB
# cp env.local.example .env
# Then start MongoDB locally and run: npm run seed:admin && npm run seed:proposals

PORT=3001
NODE_ENV=development

# Local MongoDB (start with: mongod or docker run -p 27017:27017 mongo)
MONGODB_URI=mongodb://localhost:27017/krm

JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=7d
JWT_COOKIE_EXPIRE=7

FRONTEND_URL=http://localhost:5173
